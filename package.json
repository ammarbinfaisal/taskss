{
  "name": "taskss",
  "version": "0.2.0",
  "description": "a todo terminal app",
  "main": "index.js",
  "dependencies": {
    "chalk": "^2.4.2",
    "configstore": "^4.0.0",
    "inquirer": "^6.2.2",
    "meow": "^5.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "husky": "*",
    "xo": "*"
  },
  "scripts": {
    "xo": "xo --fix --prettier",
    "fix": "npm run xo",
    "prettier": "prettier --write *.js utils/* lib/*",
    "format": "npm run prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix"
    }
  },
  "bin": {
    "taskss": "./cli.js"
  },
  "xo": {
    "rules": {
      "unicorn/no-process-exit": "off"
    }
  },
  "author": "Malik Ammar Faisal",
  "license": "GPL-3.0-only",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ammarbinfaisal/taskss.git"
  },
  "bugs": {
    "url": "https://github.com/ammarbinfaisal/taskss/issues"
  },
  "homepage": "https://github.com/ammarbinfaisal/taskss#readme",
  "keywords": [
    "todoapp",
    "todo",
    "terminal-based",
    "terminal-app"
  ]
}
